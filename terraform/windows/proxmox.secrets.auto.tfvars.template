# DO NOT commit the actual .tfvars file to version control

proxmox_secrets = {
  api_token = "your-proxmox-user@pve!your-token-name=your-token-uuid"
}

# Windows administrator password for domain controllers and servers
windows_admin_password = "your-secure-admin-password-here"
